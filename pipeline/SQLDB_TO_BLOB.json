					"secureInput": false
				},
				"userProperties": []
			},
			{
				"name": "WebActivity",
				"type": "WebActivity",
				"dependsOn": [
					{
						"activity": "CopyActivity",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": []
			}
		],
		"annotations": []
